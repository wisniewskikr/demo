{
    "executionRoleArn": "arn:aws:iam::381196861170:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "mvc-thymeleaf-container",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
				{
				  "hostPort": 8080,
				  "protocol": "tcp",
				  "containerPort": 8080
				},
				{
				  "hostPort": 80,
				  "protocol": "tcp",
				  "containerPort": 80
				}
			]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "family": "mvc-thymeleaf-task"
}